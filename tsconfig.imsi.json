{
    "compilerOptions": {
      "target": "es5", // target 은 javascript로 변환할때의 ECMA 버전이다
      "lib": [
        "dom",
        "dom.iterable",
        "esnext"
      ],
      "outDir": "out.test",
      "allowJs": true,
      "skipLibCheck": true,
      "esModuleInterop": true,
      "allowSyntheticDefaultImports": true,
      "strict": true,
      "forceConsistentCasingInFileNames": true,
      "noFallthroughCasesInSwitch": true,
      "module": "esnext",
      "moduleResolution": "node",
      "resolveJsonModule": true,
      "isolatedModules": true,
      "noEmit": true
    },
    "include": [
      "src.test/**/*"
    ],
    "exclude": [
    ]
}
